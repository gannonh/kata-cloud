{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Kata Cloud",
  "colors": {
    "primary": "#d4a574",
    "light": "#e8c49a",
    "dark": "#b8956a"
  },
  "favicon": "/assets/favicon.svg",
  "logo": {
    "light": "/assets/logo-header-light.svg",
    "dark": "/assets/logo-header-dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "GitHub",
        "href": "https://github.com/gannonh/kata-cloud"
      }
    ],
    "primary": {
      "label": "Download",
      "href": "https://kata.sh",
      "type": "button"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity"
    ],
    "display": "header"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "User Guide",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "introduction"
            ]
          },
          {
            "group": "User Guide",
            "pages": [
              "user-guide/getting-started",
              "user-guide/spaces",
              "user-guide/orchestrator",
              "user-guide/the-spec",
              "user-guide/changes-tab",
              "user-guide/agent-specialists",
              "user-guide/github-integration",
              "user-guide/mcp-support"
            ]
          }
        ]
      },
      {
        "tab": "Developer Docs",
        "groups": [
          {
            "group": "Developer Docs",
            "pages": [
              "developer/architecture",
              "developer/development-setup",
              "developer/project-structure",
              "developer/shell-api",
              "developer/state-management",
              "developer/testing",
              "developer/contributing"
            ]
          }
        ]
      }
    ],
    "anchors": [
      {
        "name": "GitHub",
        "icon": "github",
        "url": "https://github.com/gannonh/kata-cloud"
      },
      {
        "name": "Issues",
        "icon": "circle-exclamation",
        "url": "https://github.com/gannonh/kata-cloud/issues"
      }
    ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/gannonh/kata-cloud"
    }
  }
}

{
  "name": "kata-cloud",
  "version": "0.0.0",
  "private": true,
  "type": "commonjs",
  "packageManager": "pnpm@10.6.2",
  "scripts": {
    "dev": "node bin/dev-desktop.mjs",
    "web:dev": "vite",
    "build": "pnpm run desktop:build-main && vite build",
    "desktop:build-main": "tsc -p tsconfig.main.json",
    "desktop:typecheck": "tsc --noEmit -p tsconfig.main.json && tsc --noEmit -p tsconfig.renderer.json",
    "e2e:electron:smoke": "node bin/playwright-electron-runner.mjs --suite smoke",
    "e2e:electron:uat": "node bin/playwright-electron-runner.mjs --suite uat",
    "e2e:electron:full": "node bin/playwright-electron-runner.mjs --suite full",
    "e2e:electron": "pnpm run e2e:electron:full",
    "e2e": "pnpm run e2e:electron",
    "uat:electron:smoke": "pnpm run e2e:electron:smoke",
    "uat:electron:e2e": "pnpm run e2e:electron:uat",
    "repo:guardrails": "node bin/repo-guardrails.mjs",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.15.17",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.1",
    "electron": "^37.2.0",
    "eslint": "^9.0.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.5.0",
    "globals": "^17.3.0",
    "jsdom": "^28.1.0",
    "playwright-core": "^1.58.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "vitest": "^3.0.5",
    "wait-on": "^9.0.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "esbuild"
    ]
  }
}